{
  "name": "axxon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate:make": "KNEX_MIGRATION_EXTENSION=ts knex migrate:make --knexfile knexfile.ts",
    "migrate:latest": "ts-node --esm ./node_modules/knex/bin/cli.js migrate:latest --knexfile knexfile.ts",
    "seed": "ts-node --esm ./node_modules/knex/bin/cli.js seed:run --knexfile ./knexfile.ts --env development",
    "rollback": "ts-node --esm ./node_modules/knex/bin/cli.js migrate:rollback --knexfile ./knexfile.ts --env development",
    "redis:start": "docker compose up -d redis",
    "redis:stop": "docker compose down",
    "redis:logs": "docker compose logs -f redis",
    "redis:cli": "docker exec -it axxon-redis redis-cli"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@sentry/nextjs": "^9.29.0",
    "@tanstack/query-async-storage-persister": "^5.83.0",
    "@tanstack/query-sync-storage-persister": "^5.83.0",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-persist-client": "^5.83.0",
    "@tanstack/react-router": "^1.121.2",
    "axios": "^1.9.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "jose": "^6.0.12",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.539.0",
    "next": "15.1.6",
    "next-auth": "^4.24.11",
    "pg": "^8.16.0",
    "pg-query-stream": "^4.10.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-window": "^1.8.11",
    "sqlite3": "^5.1.7",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "knex": "^3.1.0",
    "postcss": "^8.5.5",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.2",
    "typescript": "^5"
  }
}
